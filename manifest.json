{
  "manifest_version": 2,
  "name": "What's In The News",
  "version": "0.1.0",
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["scripts/utils.js", "scripts/content.js"]
  }],
  "browser_action": {
    "default_icon": {
      "16": "icons/icon_16.png",
      "32": "icons/icon_32.png",
      "48": "icons/icon_48.png",
      "128": "icons/icon_128.png"
    }
  },
  "background": {
    "scripts": ["scripts/background.js"]
  }
}
